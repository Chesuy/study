#define _CRT_SECURE_NO_WARNINGS 1

#include <stdio.h>
#include <math.h>


int main()//打印9*9乘法表
{
	int i = 0;
	for (i = 1; i < 10; i++)
	{
		int j = 1;
		for (j = 1; j <= i; j++)
		{
			printf("%d*%d=%-2d  ", i, j, i * j);
		}
		printf("\n\n");
	}

	return 0;
}
